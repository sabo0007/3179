{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 720,
  "height": 520,
  "title": {
    "subtitle": "Department of Education â€” Equivalent Full-Time Student Load; tooltip shows change vs 2023"
  },
  "projection": { "type": "mercator", "center": [134, -25], "scale": 900 },
  "layer": [
    { "data": { "sphere": {} }, "mark": { "type": "geoshape", "fill": "#eef4fa" } },
    { "data": { "graticule": { "step": [30, 30] } }, "mark": { "type": "geoshape", "stroke": "#d7dde5", "strokeWidth": 0.6 } },
    {
      "data": { "url": "data/aus-states.topojson", "format": { "type": "topojson", "feature": "states" } },
      "transform": [
        {
          "lookup": "properties.STE_NAME21",
          "from": { "data": { "url": "data/eftsl_by_state.csv" }, "key": "State", "fields": ["EFTSL", "Change_Percent"] }
        }
      ],
      "mark": { "type": "geoshape" },
      "encoding": {
        "color": {
          "field": "EFTSL",
          "type": "quantitative",
          "legend": { "title": "EFTSL (2024)" },
          "scale": { "scheme": "blues" }
        },
        "tooltip": [
          { "field": "properties.STE_NAME21", "title": "State/Territory" },
          { "field": "EFTSL", "title": "EFTSL (2024)", "format": "," },
          { "field": "Change_Percent", "title": "Change vs 2023", "format": ".1f" }
        ]
      }
    },
    {
      "data": { "url": "data/aus-states.topojson", "format": { "type": "topojson", "feature": "states" } },
      "mark": { "type": "geoshape", "fill": null, "stroke": "white", "strokeWidth": 1 }
    }
  ],
  "config": {
    "view": { "stroke": null },
    "axis": {
      "labelFont": "Inter, Segoe UI, Roboto, Arial, sans-serif",
      "titleFont": "Inter, Segoe UI, Roboto, Arial, sans-serif",
      "labelColor": "#0b1520",
      "titleColor": "#0b1520"
    },
    "legend": {
      "labelFont": "Inter, Segoe UI, Roboto, Arial, sans-serif",
      "titleFont": "Inter, Segoe UI, Roboto, Arial, sans-serif"
    },
    "title": {
      "font": "Inter, Segoe UI, Roboto, Arial, sans-serif",
      "color": "#0b1520",
      "anchor": "start",
      "fontSize": 18,
      "subtitleFontSize": 12,
      "subtitleColor": "#475467"
    }
  }
}